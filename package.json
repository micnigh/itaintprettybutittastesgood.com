{
  "description": "itaintprettybutittastesgood.com static site generator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:e2e:install": "pnpm exec playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "fetch-data": "tsx scripts/fetch-data.ts",
    "fetch-data:clean": "tsx scripts/fetch-data.ts --no-cache",
    "predeploy": "npm run build && echo \"itaintprettybutittastesgood.com\" > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "pnpm predeploy && gh-pages -d dist"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@headlessui/react": "^2.2.9",
    "fraction.js": "^5.3.4",
    "googleapis": "^167.0.0",
    "js-yaml": "^4.1.1",
    "node-fetch": "^3.3.2",
    "p-queue": "^9.0.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-image-optimizer": "^2.0.3"
  },
  "packageManager": "pnpm@10.16.1"
}
