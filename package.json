{
  "description": "itaintprettybutittastesgood.com static site generator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:e2e:install": "pnpm exec playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "fetch-data": "tsx scripts/fetch-data.ts",
    "fetch-data:clean": "tsx scripts/fetch-data.ts --no-cache",
    "predeploy": "npm run build && echo \"itaintprettybutittastesgood.com\" > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "pnpm predeploy && gh-pages -d dist"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@headlessui/react": "^2.2.8",
    "fraction.js": "^5.3.4",
    "googleapis": "^159.0.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "p-queue": "^8.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.1",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.4.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.43.0",
    "vite": "^5.2.0",
    "vite-plugin-image-optimizer": "^2.0.2"
  },
  "packageManager": "pnpm@10.16.1"
}
