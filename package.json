    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sharp": "^5.5.0",
    "gatsby-plugin-theme-ui": "^0.15.4",
    "gatsby-plugin-webfonts": "^2.3.2",
    "gatsby-remark-gifs": "^1.1.0",
