    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sharp": "^5.12.1",
    "gatsby-plugin-theme-ui": "^0.16.1",
    "gatsby-plugin-webfonts": "^2.3.2",
    "gatsby-remark-gifs": "^1.2.0",
