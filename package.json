{
  "description": "itaintprettybutittastesgood.com static site generator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:unit": "vitest run --config vitest.unit.config.ts --reporter=verbose",
    "test:integration": "vitest run --config vitest.integration.config.ts --reporter=verbose",
    "test:e2e:install": "pnpm exec playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "fetch-data": "tsx scripts/fetch-data.ts",
    "fetch-data:clean": "tsx scripts/fetch-data.ts --no-cache",
    "validate": "pnpm lint && pnpm typecheck && pnpm format && pnpm test",
    "predeploy": "npm run build && echo \"itaintprettybutittastesgood.com\" > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "pnpm predeploy && gh-pages -d dist"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@headlessui/react": "^2.2.9",
    "fraction.js": "^5.3.4",
    "framer-motion": "^12.23.26",
    "googleapis": "^169.0.0",
    "js-yaml": "^4.1.1",
    "node-fetch": "^3.3.2",
    "p-queue": "^9.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "react-tooltip": "^5.30.0",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.16",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa"
}
